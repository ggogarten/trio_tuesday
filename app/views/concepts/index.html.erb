<p id="notice"><%= notice %></p>

<h1>Listing Concepts</h1>

<table>
  <thead>
    <tr>
      <th>Idea</th>
      <th>Relevance</th>
      <th>Speech</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @concepts.each do |concept| %>
      <tr>
        <td><%= concept.idea %></td>
        <td><%= concept.relevance %></td>
        <td><%= concept.speech %></td>
        <td><%= link_to 'Show', concept %></td>
        <td><%= link_to 'Edit', edit_concept_path(concept) %></td>
        <td><%= link_to 'Destroy', concept, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Watson', 'analyze' %>
